
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc@0/c000000.wifi'
	option band '5g'
	option channel 'auto'
	option htmode 'HE20'
	option country 'US'
	option cell_density '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc@0/c000000.wifi+1'
	option band '2g'
	option channel 'auto'
	option htmode 'HE20'
	option country 'US'
	option cell_density '0'

config wifi-device 'radio2'
	option type 'mac80211'
	option path 'platform/soc@0/c000000.wifi+2'
	option band '5g'
	option channel '149'
	option htmode 'HE20'
	option country 'US'
	option cell_density '0'

config wifi-iface 'wifinet1'
	option device 'radio0'
	option mode 'ap'
	option ssid 'SSID_PRIMARY_5G'
	option encryption 'sae-mixed'
	option key 'REPLACE_ME'
	option ocv '0'
	option network 'home'

config wifi-iface 'wifinet2'
	option device 'radio1'
	option mode 'ap'
	option ssid 'SSID_PRIMARY_2G'
	option encryption 'sae-mixed'
	option key 'REPLACE_ME'
	option ocv '0'
	option network 'home'

